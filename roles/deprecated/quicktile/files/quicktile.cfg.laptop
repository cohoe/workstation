[general]
cfg_schema = 1
UseWorkarea = True
ModMask = <Ctrl><Alt>

[keys]
C = move-to-center
H = horizontal-maximize
V = vertical-maximize
KP_0 = maximize
2 = bottom-left
Down = bottom
3 = bottom-right
Left = left
KP_5 = middle
Right = right
1 = top-left
Up = top
4 = top-right
KP_Enter = monitor-switch

